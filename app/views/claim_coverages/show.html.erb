

<div class="card">
  <div class="card-header">
    <h1>Claim coverage</h1>
    <%= link_to "Delete", claim_coverage, class: "btn btn-danger btn-sm", method: :delete, data: { "turbo-method": :delete, turbo_confirm: "Are you sure?" } %>
  </div>
  <div class="card-body">
    <%= render @claim_coverage %>
  </div>
  <div class="card-footer">
    <%= link_to "Edit", edit_claim_coverage_path(@claim_coverage), class: "btn btn-secondary btn-sm" %> |
    <%= link_to "Back", claim_coverages_path, class: "btn btn-secondary btn-sm" %>
  </div>

</div>
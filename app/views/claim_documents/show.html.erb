

<div class="card">
  <div class="card-header">
    <h1>Claim document</h1>
    <%= link_to "Delete", claim_document, class: "btn btn-danger btn-sm", method: :delete, data: { "turbo-method": :delete, turbo_confirm: "Are you sure?" } %>
  </div>
  <div class="card-body">
    <%= render @claim_document %>
  </div>
  <div class="card-footer">
    <%= link_to "Edit", edit_claim_document_path(@claim_document), class: "btn btn-secondary btn-sm" %> |
    <%= link_to "Back", claim_documents_path, class: "btn btn-secondary btn-sm" %>
  </div>

</div>